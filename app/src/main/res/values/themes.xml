<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AminAndroid.Light" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
        <item name="listItemBackground">@drawable/background_listitem_light</item>
        <item name="lineListItemBackground">@drawable/background_listitemline_light</item>
    </style>
    <style name="Theme.AminAndroid.Dark" parent="Theme.AppCompat">
        <item name="listItemBackground">@drawable/background_listitem_dark</item>
        <item name="lineListItemBackground">@drawable/background_listitemline_dark</item>
        <item name="colorPrimary">@color/darkPrimaryColor</item>
        <item name="colorPrimaryDark">@color/darkPrimaryDarkColor</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <!-- Add more styling attributes as needed -->
    </style>

    <style name="Theme.AminAndroid" parent="Base.Theme.AminAndroid" />
    <style name="RoundCorner" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">10dp</item>
        <item name="color">@color/white</item>
    </style>
</resources>